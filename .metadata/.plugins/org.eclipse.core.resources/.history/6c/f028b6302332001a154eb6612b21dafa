package projetStatic;

public class Personne {

	
	private String nom;
	private String prenom;
	
	
	
	//constructeur
	public Personne(String nom, String prenom) {
		this.nom = nom;
		this.prenom = prenom;
	}


	//methode toString
	
	public String toString() {
		return "Personne [nom=" + nom + ", prenom=" + prenom + "]";
	}
	
	
}
