	static void test4() {
		Personne p = new Personne("dupond ", "toto ");// création d'une personne
														// [newPersonne()]=constructeur
		System.out.println(p);
		p.setNom("durant");
		// System.out.println(p.getPrenom());
		System.out.println(p);
		p.setNom("Anis");
		System.out.println(p);
	}

	static void test3() {
		Personne p = new Personne("dupond ", "toto"); // création d'une
														// personne

		p.setAge(30);
		p.getAge();
		System.out.println(p);

		System.out.println(p.toString());// appel explicite

	}

	static void test2() {
		Personne p = new Personne("dupond ", "toto ", 10);
		System.out.println(p.toString());
		System.out.println(p);
		p.setAge(55);
		System.out.println(p.toString());// appel explicite

	}

	static void test1() {
		Personne p = new Personne("dupond ", "toto ", 10); // création d'une new
															// personne
															// constructeur

		System.out.println(p.toString());
		// System.out.println(p);
