package model;

public class Bateau {
	private String marque;
	private int prix;
	private String EBateau;
	
	
	public Bateau(String marque, int prix, String eBateau) {
		this.marque = marque;
		this.prix = prix;
		EBateau = eBateau;
	}



	public String toString() {
		return "Bateau [marque=" + marque + ", prix=" + prix + ", EBateau=" + EBateau + "]";
	}
	
	
	
	
	
	

}
