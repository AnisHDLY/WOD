package projetenumere;

public enum Formation {
	
	java(500),
	python(300),
	cpp(100),
	html(0),
	csharp(5); // point virgule
	
	private int prix;
	
	public int getPrix(){
		return prix;
	}
	
	private Formation (int prix){ // constructeur par defaut
	this.prix=prix;
	}
	
	private Formation() {
	
	}
}
