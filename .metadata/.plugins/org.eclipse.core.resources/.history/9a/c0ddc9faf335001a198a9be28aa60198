package model;

public class TroisD extends DeuxD {

	private int z;

	public TroisD (int x, int y, int z) // on doit remettre les attribus en protected
	this.x=x;
	this.y=y;// ou super(x,y) constructeur de la mère
	this.z=z;

	public String toString() {
		return super.toString() + " z" + z; // "DeuxD [x=" + x + ", y=" + y +
											// "]";
	}

}
