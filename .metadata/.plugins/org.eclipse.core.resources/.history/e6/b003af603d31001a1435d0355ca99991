package model;

public class Personne {

	
	private String nom;
	private String prenom;
	private Adress adress;
	
	public Personne(String nom, String prenom, Adress adress) {
		this.nom = nom;
		this.prenom = prenom;
		this.adress = adress;
	}

	
	public String toString() {
		return "Personnev5 [nom=" + nom + ", prenom=" + prenom;
		reponse+= (adresse!==null)?  "\nadress=" + adress.toString() + ;"";
		
	}
	
	
	
}
